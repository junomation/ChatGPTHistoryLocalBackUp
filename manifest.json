{
    "manifest_version": 3,
    "name": "Web Page Backup",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "storage",
        "fileSystem"
      ],
      "page_action": {
        "default_popup": "popup.html",
        "show_matches": ["https://chat.openai.com/*"]
      },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  